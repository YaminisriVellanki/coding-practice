import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
    Scanner sc = new Scanner(System.in);
    int N = sc.nextInt();
    int X = sc.nextInt();
		int [] A = new int[N];
		int c=0;
		for(int i = 0;i<N;i++)
		{
		  A[i]=sc.nextInt();
		  if(A[i]==X)
		  {
		    c++;
		    break;
		  }
		  
		}
		if(c==1)
		{
		  System.out.println("YES");
		}
		else{
		  System.out.println("NO");
		}
	}
}
